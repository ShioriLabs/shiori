{
  "name": "@shiorilabs/shiori",
  "version": "0.0.1",
  "description": "Yet another Discord bot",
  "main": "dist/app.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "unit": "nyc ava",
    "build": "rm -rf dist && tsc",
    "dev": "NODE_ENV=development nodemon -e ts --watch ./src --exec \"babel-node src/app.ts --extensions=\".ts\"\"",
    "start": "node dist/app.js",
    "version": "npm run build && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShioriLabs/shiori.git"
  },
  "author": "Dhika Rizky <deka_r@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShioriLabs/shiori/issues"
  },
  "homepage": "https://github.com/ShioriLabs/shiori#readme",
  "dependencies": {
    "@sentry/node": "^6.3.0",
    "@sentry/tracing": "^6.3.0",
    "axios": "^0.21.1",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "faunadb": "^2.14.2",
    "ffmpeg-static": "^4.2.7",
    "jimp": "^0.16.1",
    "jsqr": "^1.3.1",
    "qrcode-reader": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.9.3",
    "@types/yt-search": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "ava": "^3.15.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
